<div class="breadcrumb">
    <div class="container">
        <a class="breadcrumb-item" href="/">User Page</a>
        <span class="breadcrumb-item active">Add Address</span>
    </div>
</div>
<section class="static about-sec">
    <div class="container">
        <h1>Add Address</h1>
        <h3></h3>
        <div class="form">
            <div class="row">
                <div class="col-3">
                </div>
                <div class="col-md-6">
                    <form method="POST" action="/user/userpage/changeaddress">
                        <!--- add in forms here--->
                        <div class="form-group">
                            <label for="PhoneNo">Phone Number</label>
                            <input  name="PhoneNo" class="form-control">
                        </div>
                        <div class="form-group">
                            <label for="address">Address Line 1</label>
                            <input  name="address" class="form-control">
                        </div>
                        <div class="form-group">
                            <label for="address1"> Address Line 2</label>
                            <input  name="address1" class="form-control">
                        </div>
                        <div class="form-group">
                            <label for="city">City</label>
                            <input  name="city" class="form-control">
                        </div>
                        <div class="form-group">
                            <label for="country">Country</label>
                            <input  name="country" class="form-control">
                        </div>
                        <div class="form-group">
                            <label for="postalCode">Postal Code</label>
                            <input  name="postalCode" class="form-control">
                        </div>
                        <div class="row">
                            <div>
                                <button class="btn black" type="submit">Save</button>
                            </div>
                        </div>                   
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>